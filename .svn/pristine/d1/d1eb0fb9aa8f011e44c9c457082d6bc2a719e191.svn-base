package com.gd.entity;

import java.util.Date;

public class Announce {
	private long anno_id;
	private String anno_content;
	private int anno_property;
	private Date anno_time;
	public Announce() {
		super();
		// TODO Auto-generated constructor stub
	}
	public long getAnno_id() {
		return anno_id;
	}
	public void setAnno_id(long anno_id) {
		this.anno_id = anno_id;
	}
	public String getAnno_content() {
		return anno_content;
	}
	public void setAnno_content(String anno_content) {
		this.anno_content = anno_content;
	}
	public int getAnno_property() {
		return anno_property;
	}
	public void setAnno_property(int anno_property) {
		this.anno_property = anno_property;
	}
	public Date getAnno_time() {
		return anno_time;
	}
	public void setAnno_time(Date anno_time) {
		this.anno_time = anno_time;
	}
	@Override
	public String toString() {
		return "Announce [anno_id=" + anno_id + ", anno_content="
				+ anno_content + ", anno_property=" + anno_property
				+ ", anno_time=" + anno_time + "]";
	}
	
}
