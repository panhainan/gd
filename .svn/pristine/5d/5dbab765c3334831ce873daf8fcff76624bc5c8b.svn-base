package com.gd.entity;

import java.util.Date;
import java.util.List;

public class Question {
	private long ques_id;
	private User ques_user_send;
	private User ques_user_rece;
	private List<Question> ques_area_ques;
	private Idea ques_idea;
	private String ques_content;
	private Date ques_time;
	private Question ques_rped_ques;
	private boolean ques_rece_read;
	public Question() {
		super();
	}
	
	public long getQues_id() {
		return ques_id;
	}

	public void setQues_id(long ques_id) {
		this.ques_id = ques_id;
	}

	public User getQues_user_send() {
		return ques_user_send;
	}

	public void setQues_user_send(User ques_user_send) {
		this.ques_user_send = ques_user_send;
	}

	public User getQues_user_rece() {
		return ques_user_rece;
	}

	public void setQues_user_rece(User ques_user_rece) {
		this.ques_user_rece = ques_user_rece;
	}

	public List<Question> getQues_area_ques() {
		return ques_area_ques;
	}

	public void setQues_area_ques(List<Question> ques_area_ques) {
		this.ques_area_ques = ques_area_ques;
	}

	public Idea getQues_idea() {
		return ques_idea;
	}

	public void setQues_idea(Idea ques_idea) {
		this.ques_idea = ques_idea;
	}

	public String getQues_content() {
		return ques_content;
	}

	public void setQues_content(String ques_content) {
		this.ques_content = ques_content;
	}

	public Date getQues_time() {
		return ques_time;
	}

	public void setQues_time(Date ques_time) {
		this.ques_time = ques_time;
	}

	public Question getQues_rped_ques() {
		return ques_rped_ques;
	}

	public void setQues_rped_ques(Question ques_rped_ques) {
		this.ques_rped_ques = ques_rped_ques;
	}

	public boolean isQues_rece_read() {
		return ques_rece_read;
	}

	public void setQues_rece_read(boolean ques_rece_read) {
		this.ques_rece_read = ques_rece_read;
	}

	@Override
	public String toString() {
		return "Question [ques_id=" + ques_id + ", ques_user_send="
				+ ques_user_send + ", ques_user_rece=" + ques_user_rece
				+ ", ques_area_ques=" + ques_area_ques + ", ques_idea="
				+ ques_idea + ", ques_content=" + ques_content + ", ques_time="
				+ ques_time + ", ques_rped_ques=" + ques_rped_ques
				+ ", ques_rece_read=" + ques_rece_read + "]";
	}

	
	
	
}
