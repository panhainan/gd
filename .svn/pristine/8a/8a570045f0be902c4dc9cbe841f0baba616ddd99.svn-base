package com.gd.entity;

import java.util.Date;

public class Letter {
	
	private long lett_id;
	private User lett_user_send;
	private User lett_user_rece;
	private String lett_content;
	private Date lett_time;
	private boolean lett_rece_read;
	public Letter() {
		super();
	}
	public long getLett_id() {
		return lett_id;
	}
	public void setLett_id(long lett_id) {
		this.lett_id = lett_id;
	}
	public User getLett_user_send() {
		return lett_user_send;
	}
	public void setLett_user_send(User lett_user_send) {
		this.lett_user_send = lett_user_send;
	}
	public User getLett_user_rece() {
		return lett_user_rece;
	}
	public void setLett_user_rece(User lett_user_rece) {
		this.lett_user_rece = lett_user_rece;
	}
	public String getLett_content() {
		return lett_content;
	}
	public void setLett_content(String lett_content) {
		this.lett_content = lett_content;
	}
	public Date getLett_time() {
		return lett_time;
	}
	public void setLett_time(Date lett_time) {
		this.lett_time = lett_time;
	}
	
	public boolean isLett_rece_read() {
		return lett_rece_read;
	}
	public void setLett_rece_read(boolean lett_rece_read) {
		this.lett_rece_read = lett_rece_read;
	}
	@Override
	public String toString() {
		return "Letter [lett_id=" + lett_id + ", lett_user_send="
				+ lett_user_send + ", lett_user_rece=" + lett_user_rece
				+ ", lett_content=" + lett_content + ", lett_time=" + lett_time
				+ ", lett_rece_read=" + lett_rece_read + "]";
	}
	
}
